<view class="meeting">
  <view class="meeting_date">
    {{nowDate.yearData}}年{{nowDate.monthData}}月{{nowDate.dayData}}日 星期{{nowDate.weekData}}
  </view>
  <view class="meeting_room" wx:for="(item, index) in meetingRoom" wx:key="index">
    <view class="item_left">
      <text class="title">{{meetingRoom[index].name}}</text>
      <text class="describe">{{meetingRoom[index].office}}{{meetingRoom[index].floor}}楼  可容纳：{{meetingRoom[index].num}}人</text>
    </view>
    <view class="item_right">
      <van-button round type="info" size="small" color="#d81e06" bind:click="goReserve">立即预约</van-button>
    </view>
    <!-- <van-icon name="arrow-left" bind:click="toprev" />
    <text class="meeting_room_text">{{meetingRoom[roomIndex].name}}</text>
    <van-icon name="arrow" bind:click="tonext" /> -->
  </view>
</view>